<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>dashboard</title>
</head>
<body>
    <p><a href="/tambahdata">tambah data</a></p>
    <table border="1" cellspacing="0" cellpadding="5">
      <thead>
        <th>id_user</th>
        <th>username</th>
        <th>email</th>
        <th>nama</th>
        <th>alamat</th>
        <th>Update</th>
        <th>Delete</th>
      </thead>
      <tbody>   
        <tbody>   
      <% let i = 1 %> 
        <tr>
        <% data.forEach((data)=>{ %>     
          <td><%= i %> </td>
          <td><%=data.id_user %></td>
          <td><%=data.username %></td>
          <td><%=data.email %></td>
          <td><%=data.nama %></td>
          <td><%=data.alamat %></td>
          <td><a href="/user_game/update/<%= data.id  %> ">update</a> </td>
          <td><a href="/user_game/delete/<%= data.id %> ">hapus</a> </td>  
          <% i++ %> 
        </tr>      
      <% }) %>
    </tbody>
  </table>
</body>
</html>